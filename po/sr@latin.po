#: ../src/system-config-services.glade:1280
msgid "Customize Runlevels"
msgstr "Prilagodi izvršne nivoe"

#: ../src/system-config-services.glade:1351 ../src/gui.py:714
#, python-format
msgid "Enable the <b>%(service)s</b> service in these runlevels:"
msgstr "Uključi servis <b>%(service)s</b> u ovim izvršnim nivoima:"

#: ../src/system-config-services.glade:1395
msgid "Runlevel _2"
msgstr "Izvršni nivo _2"

#: ../src/system-config-services.glade:1414
msgid "Runlevel _3"
msgstr "Izvršni nivo _3"

#: ../src/system-config-services.glade:1433
msgid "Runlevel _4"
msgstr "Izvršni nivo _4"

#: ../src/system-config-services.glade:1452
msgid "Runlevel _5"
msgstr "Izvršni nivo _5"

#: ../src/gui.py:112
msgid "Name"
msgstr "Ime"

#: ../src/gui.py:113
msgid "Remarks"
msgstr "Napomene"

#: ../src/gui.py:174 ../src/gui.py:192
msgid "This service is being refreshed right now."
msgstr "Ovaj se servis osvežava sada."

#: ../src/gui.py:175
msgid "Getting information about this service failed."
msgstr "Pribavljanje informacija o ovom servisu nije uspelo."

#: ../src/gui.py:176
msgid "This service is enabled."
msgstr "Ovaj servis je uključen."

#: ../src/gui.py:177
msgid "This service is disabled."
msgstr "Ovaj servis je isključen."

#: ../src/gui.py:178
#, python-format
msgid "This service is enabled in runlevels: %(runlevels)s"
msgstr "Servis je uključen u izvršnim nivoima: %(runlevels)s"

#: ../src/gui.py:193
msgid "The status of this service is unknown."
msgstr "Status ovog servisa je nepoznat."

#: ../src/gui.py:194
msgid "This service is stopped."
msgstr "Ovaj servis je zaustavljen."

#: ../src/gui.py:195
msgid "This service is running."
msgstr "Ovaj servis je pokrenut."

#: ../src/gui.py:196
msgid "This service is dead."
msgstr "Ovaj servis ne reaguje."

#: ../src/gui.py:273
#, python-format
msgid ""
"The <b>%(servicename)s</b> service is started once, usually when the system "
"is booted, runs in the background and wakes up when needed."
msgstr ""
"Servis <b>%(servicename)s</b> se pokreće jednom, obično prilikom podizanja "
"sistema, radi u pozadini i budi se po potrebi."

#: ../src/gui.py:287
msgid "This service is updated currently."
msgstr "Ovaj servis je se trenutno ažurira."

#: ../src/gui.py:313
#, python-format
msgid ""
"The <b>%(servicename)s</b> service will be started on demand by the xinetd "
"service and ends when it has nothing more to do."
msgstr ""
"Servis xinetd će pokrenuti servis <b>%(servicename)s</b> na zahtev i "
"završiće kada ne bude imao više šta da radi."

#: ../src/gui.py:322
msgid ""
"This service is enabled, but the <b>xinetd</b> package is not installed. "
"This service does not work without it."
msgstr ""
"Ovaj servis je uključen, ali paket <b>xinetd</b> nije instaliran. Ovaj "
"servis ne radi bez njega."

#: ../src/gui.py:328
msgid ""
"This service is enabled, but the <b>xinetd</b> service is not running. This "
"service does not work without it."
msgstr ""
"Ovaj servis je uključen, ali servis <b>xinetd</b> nije pokrenut. Ovaj servis "
"ne radi bez njega."

#: ../src/gui.py:846
msgid ""
"The help viewer could not be found. To be able to view help, the 'yelp' "
"package needs to be installed."
msgstr ""
"Program za pregledanje pomoći nije nađen. Da biste mogli pregledati pomoć "
"paket „yelp“ mora biti instaliran."

